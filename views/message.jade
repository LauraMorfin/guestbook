extends layout

block content
    h1.
     Message

    table.table
     thead
      tr
       td Name
       td Email
       td Comments
       td
     tbody
      each key, i in usermessage
       tr
        td #{key.username}
        td #{key.email}
        td #{key.comment}
        td
         a.btn.btn-info(href="/delete/#{key._id}") Delete
